;\n\nfunction App() {\n  const [color, setColor] = React.useState(\"green\");\n\n  return <h1 style={{ \"color\" : color }}>This is HOME!!!</h1>;\n}\n\nexport default App;\n",["92","93"],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/components/AppBar.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/components/Admin.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/components/TodoListWeb.js",["94","95","96"],"\nimport React, { useState,useEffect } from 'react';\n\nimport Typography from '@material-ui/core/Typography';\nimport TodoForm from '../todolist/TodoForm';\nimport TodoList from '../todolist/TodoList';\nimport useTodoState from '../todolist/useTodoState';\n\nimport '../todolist/todolist.css';\n\nexport default function TodoListWeb({todos,addTodo,deleteTodo}){\n\n  // const [isDisabled, setIsDisabled] = useState(false); // Set Begin Value\n  \n  // Tao State cho isColor \n  // const [isColor, setIsColor] = useState(true)\n\n\n  return (\n    <div className='App'>    \n      <Typography className='h1' component='h1' variant='h2'>\n            Todos\n            <span>Get things done, one item at a time.</span>\n        </Typography>\n\n      <TodoForm\n        saveTodo={todoText => {\n          const trimmedText = todoText.trim();\n\n          if (trimmedText.length > 0) {\n            addTodo(trimmedText);\n          }\n        }}\n      />\n\n      <TodoList todos={todos} deleteTodo={deleteTodo} />\n    </div>\n  );\n};","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/components/About.js",["97","98","99","100","101","102","103","104","105","106"],"import React from 'react';\nimport '../about/about.css';\n\nconst About = () => (\n\t<div className=\"wrapper\">\n\t\t<div className=\"gallery\">\n\t\t\t<ul>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/9.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/2.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/3.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/1.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/4.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/5.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/7.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/8.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/6.png\" /></li>\n\t\t\t</ul>\n\t\t</div>\n\t\t<p class=\"attribution\">Images featured in this demo are the works of <a href=\"https://d.hatena.ne.jp/koochinko\">Mernan Behairi</a>. Inspired by an old post of <a href=\"https://twitter.com/SohTanaka\">@Sohtanaka</a>. Design Shopping Web by <a target=\"_blank\" href=\"https://github.com/phatnguyen1006\">Phat Nguyen</a>.</p>\n\t</div>\n)\n\nexport default About;","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/private/PrivateRoute.js",["107"],"import React, { useState } from 'react';\nimport { Redirect, Route, useLocation } from 'react-router-dom';\n// fakeAuth from SignIn\nimport { fakeAuth } from '../features/Auth/SignIn';\n\nconst PrivateRoute = ({ component: Component, ...rest}) => {\n    const location = useLocation();\n\n    return (\n        // If fakeAuth = true => Component Admin : else => Redirect to SignIn\n        <Route {...rest} >\n            {fakeAuth.isAuthenticated ? \n                (<Component />) :\n                (< Redirect to={{ pathname: \"/signin\", state: { from: location } }} />)\n            }\n        </Route>\n    )\n}\n\nexport default PrivateRoute;","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/components/Products.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/context/BarTheme.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/todolist/useTodoState.js",["108"],"import { useState } from 'react';\n\nexport default initialValue => {\n  const [todos, setTodos] = useState(initialValue);\n\n  return {\n    todos,\n    addTodo: todoText => {\n      setTodos([...todos, todoText]);\n    },\n    deleteTodo: todoIndex => {\n      const newTodos = todos.filter((_, index) => index !== todoIndex);\n\n      setTodos(newTodos);\n    }\n  };\n};\n","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/context/ReferenceContext.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/features/Auth/SignIn.js",["109","110","111","112"],"import React, { useEffect, useContext } from 'react';\nimport { Redirect, useLocation } from 'react-router-dom';\n// Import Firebase & FirebaseUI\nimport firebase from 'firebase';\nimport StyledFirebaseAuth from 'react-firebaseui/StyledFirebaseAuth';\n\n// Redirect To Reference:\nimport { ReferenceContext } from '../../context/ReferenceContext';\n\nimport '../../index.css';\n\n// configure FirebaseUI\nconst uiConfig = {\n  signInFlow: 'redirect',\n  // We will display Google as auth providers.\n  signInSuccessUrl: '/',\n  signInOptions: [\n    firebase.auth.GoogleAuthProvider.PROVIDER_ID,\n    firebase.auth.FacebookAuthProvider.PROVIDER_ID,\n  ],\n  // callbacks: {\n  //   // Avoid redirects after sign-in.\n  //   signInSuccessWithAuthResult: () => false,\n  // },\n};\n\nconst SignIn = () => {\n\n  const { state } = useLocation();\n  const from = state || { from: { pathname: \"/\" } };\n  const { redirect, setRedirect } = useContext(ReferenceContext);\n  // console.log(\"Redirect: \", redirect);\n  \n  if (redirect) {\n    <Redirect to={from} />\n  }\n\n  useEffect(() => {\n    if (from.from.pathname == \"/admin\") {\n      window.alert(\"You must to login first\");\n    }\n    // console.log(\"from:  \", from.from.pathname );\n  }, []);\n  \n\n  // Main return SignIn page => OutSide to easier.\n  return (\n    <div className=\"group-firebase-button\">\n      <h1>SignIn Google</h1>\n      <p>One Touch, Million Connect</p>\n      <div>\n      <StyledFirebaseAuth uiConfig={uiConfig} firebaseAuth={firebase.auth()} />\n      </div>\n      <p>/* Login to use Admin Page */</p>\n    </div>\n  );\n};\n\nexport const fakeAuth = {\n  isAuthenticated: false,\n  authenticate(callback) {\n    this.isAuthenticated = true;\n    setTimeout(callback(), 100); // Wait 0s to not delay.\n  }\n}\n\nexport default SignIn;","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/features/Auth/SignOut.js",["113"],"import React from 'react';\nimport firebase from 'firebase';\nimport { fakeAuth } from './SignIn';\n\n// SignOut Features\nconst SignOut = () => {\n    firebase.auth().signOut()\n        .then(() => {\n            console.log(\"LogOut Successed!\")\n            fakeAuth.isAuthenticated = false;\n            return;\n        })\n        .catch(() => {\n            window.confirm(\"Sorry, Something Was Wrong!\");\n        })\n}\n\nexport default SignOut;","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/todolist/TodoList.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/todolist/TodoForm.js",["114"],"import React, { useContext } from 'react';\nimport TextField from '@material-ui/core/TextField';\nimport useInputState from './useInputState';\n\n\nconst TodoForm = ({ saveTodo }) => {\n  const { value, reset, onChange } = useInputState();\n\n  return (\n    <form\n      onSubmit={event => {\n        event.preventDefault();\n\n        saveTodo(value);\n        reset();\n      }}>\n      <TextField\n        className='TextField'\n        variant='outlined'\n        placeholder='Add todo'\n        margin='normal'\n        onChange={onChange}\n        value={value}\n      />\n    </form>\n  );\n};\n\nexport default TodoForm;\n","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/product/Product.js",["115","116"],"import React from 'react';\nimport {\n    useParams,\n    useRouteMatch,\n    useHistory\n} from 'react-router-dom';\nimport './product.css';\n\nconst Product = ({data}) => {\n    \n    const url = useRouteMatch();\n    const { productId } = useParams();\n    const history = useHistory();\n    // console.log(url);\n    // console.log(product);\n\n    const handleHistory = () => {\n        history.replace(\"/products\")\n    }\n\n    // Find data by index\n    const product = data.find((p) => p.id === Number(productId) );\n    let productData;\n\n    if (product) {\n        productData = (\n            <div>\n                <h3>{product.name}</h3>\n                <div>\n                    <p>{product.description}</p>\n                    <hr />\n                    <h5>{product.status}</h5>\n                </div>\n            </div>\n        )\n    }\n    else productData = <h2>Sorry. Product doesn't exist</h2>\n\n    return (\n    <>\n        <div style={{display: \"flex\"}}>\n            <div style={{\n                margin: \"auto\",\n                width: \"50%\"\n            }}>\n                {productData}\n                <button className=\"okBack-btn\" onClick={handleHistory}>OK and Back</button>\n            </div>\n        </div>\n            {product ? (<img style={{ boxShadow: \"10px 5px grey\",marginBottom: \"2em\" ,marginRight: \"20%\" }} width=\"30%\" height=\"auto\" src={product.img} />) : <p>No Img</p>}\n    </>\n    )\n}\n\nexport default Product;","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/buttons/backButton.js",[],"/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/todolist/useInputState.js",["117"],"import { useState } from 'react';\n\nexport default () => {\n  const [value, setValue] = useState('');\n\n  return {\n    value,\n    onChange: event => {\n      setValue(event.target.value);\n    },\n    reset: () => setValue('')\n  };\n};\n","/Users/phatnguyen/ReactJS/react-js/SHOPPING_WEB/Shopping/src/context/Theme.jsx",[],{"ruleId":"118","severity":1,"message":"119","line":50,"colu