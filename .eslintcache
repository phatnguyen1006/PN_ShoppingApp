ms,\n    useRouteMatch,\n    useHistory\n} from 'react-router-dom';\nimport './product.css';\n\nconst Product = ({data}) => {\n    \n    const url = useRouteMatch();\n    const { productId } = useParams();\n    const history = useHistory();\n    // console.log(url);\n    // console.log(product);\n\n    const handleHistory = () => {\n        history.replace(\"/products\")\n    }\n\n    // Find data by index\n    const product = data.find((p) => p.id === Number(productId) );\n    let productData;\n\n    if (product) {\n        productData = (\n            <div>\n                <h3>{product.name}</h3>\n                <div>\n                    <p>{product.description}</p>\n                    <hr />\n                    <h5>{product.status}</h5>\n                </div>\n            </div>\n        )\n    }\n    else productData = <h2>Sorry. Product doesn't exist</h2>\n\n    return (\n    <>\n        <div style={{display: \"flex\"}}>\n            <div style={{\n                margin: \"auto\",\n                width: \"50%\"\n            }}>\n                {productData}\n                <button className=\"okBack-btn\" onClick={handleHistory}>OK and Back</button>\n            </div>\n        </div>\n            {product ? (<img style={{ boxShadow: \"10px 5px grey\",marginBottom: \"2em\" ,marginRight: \"20%\" }} width=\"30%\" height=\"auto\" src={product.img} />) : <p>No Img</p>}\n    </>\n    )\n}\n\nexport default Product;","/Users/phatnguyen/ReactJS/react-js/Shopping/src/components/About.js",["72","73","74","75","76","77","78","79","80"],"import React from 'react';\nimport '../about/about.css';\n\nconst About = () => (\n\t<div className=\"wrapper\">\n\t\t<div className=\"gallery\">\n\t\t\t<ul>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/9.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/2.png\" /></li>\n\t\t\t\t<li><img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/53819/3.png\" /></li>\n